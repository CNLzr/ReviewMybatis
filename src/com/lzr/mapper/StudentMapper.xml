<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lzr.mapper.StudentMapper">
    <select id="getAll" resultType="com.lzr.entity.Student">
        select * from student
    </select>
    <select id="getById" resultType="com.lzr.entity.Student">
        select * from student where id = #{id}
    </select>
</mapper>
